{
  "name": "GOBSSlides",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Google Slides OBS Integration",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
